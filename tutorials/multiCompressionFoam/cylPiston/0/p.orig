FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1e+6;

boundaryField
{
/* Inlet */
    "(injection|inlet)(X|Y)(m|p)"
    {
        type            zeroGradient;
    }

/* Outlet */
    outlet
    {
        type            fixedValue;
        value           uniform 0.23e+6;
    }

/* Walls */
    "piston|valve(Head|Stem)|walls"
    {
        type            zeroGradient;
    }

/* ACMI patches */
    "(inner|outer)ACMI(1|2)_blockage"
    {
        type            zeroGradient;
    }
    "(inner|outer)ACMI(1|2)_couple"
    {
        type            cyclicACMI;
        value           $internalField;
    }
}

// ************************************************************************* //
